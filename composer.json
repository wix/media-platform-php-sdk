{
    "name": "wix/media-platform-php-sdk",
    "description": "The official Wix Media Platform PHP SDK",
    "keywords": [
        "wix",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://gcp.wixmp.com",
    "license": "0BSD",
    "authors": [
        {
            "name": "Wix.com",
            "homepage": "https://github.com/wix"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "~6.1",
        "firebase/php-jwt": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": { "Wix\\Mediaplatform\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Wix\\Mediaplatform\\" : "test/" }
    }
}
